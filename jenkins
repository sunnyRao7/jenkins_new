#!/usr/bin/env groovy
properties([
    [$class: 'GithubProjectProperty',
    displayName: '',
    projectUrlStr: 'https://github.com/sunnyRao7/jenkins_new.git/'],
    pipelineTriggers([githubPush()])])

properties([
  pipelineTriggers([
    upstream(
      threshold: 'SUCCESS',
      upstreamProjects: 'jenkins_new/nikhil'
    )
  ])
])
pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                sh 'pwd'
		echo "new try"
            }
        }
        stage('Test'){
            steps {
                sh 'java -version'
                
            }
        }
        stage('Deploy') {
            steps {
                sh 'ls'
                sh 'pwd'
            }
        }
    }
}
